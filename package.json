{
  "name": "ag-telemetry",
  "displayName": "Antigravity AI Quota & Usage Tracker",
  "description": "Track and monitor your Antigravity AI model quota usage and remaining limits in real-time",
  "version": "2.0.1",
  "publisher": "llegomark",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llegomark/ag-telemetry"
  },
  "bugs": {
    "url": "https://github.com/llegomark/ag-telemetry/issues"
  },
  "homepage": "https://github.com/llegomark/ag-telemetry#readme",
  "icon": "media/icon.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "antigravity",
    "ai",
    "quota",
    "telemetry",
    "usage"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agTelemetry.refreshTelemetry",
        "title": "Refresh Telemetry",
        "category": "AG Telemetry",
        "icon": "$(sync)"
      },
      {
        "command": "agTelemetry.missionBriefing",
        "title": "View Quota Status",
        "category": "AG Telemetry",
        "icon": "$(dashboard)"
      },
      {
        "command": "agTelemetry.establishLink",
        "title": "Establish Uplink",
        "category": "AG Telemetry",
        "icon": "$(radio-tower)"
      },
      {
        "command": "agTelemetry.runDiagnostics",
        "title": "Run Diagnostics",
        "category": "AG Telemetry",
        "icon": "$(debug-console)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "agTelemetryContainer",
          "title": "AG Telemetry",
          "icon": "media/sidebar-icon.svg"
        }
      ]
    },
    "views": {
      "agTelemetryContainer": [
        {
          "id": "agTelemetrySystemsView",
          "name": "System Status",
          "contextualTitle": "AG Telemetry",
          "icon": "$(dashboard)",
          "visibility": "visible"
        },
        {
          "id": "agTelemetryFuelView",
          "name": "Model Quota",
          "contextualTitle": "AG Telemetry",
          "icon": "$(beaker)",
          "visibility": "visible"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "agTelemetry.refreshTelemetry",
          "when": "view =~ /agTelemetry/",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "AG Telemetry",
      "properties": {
        "agTelemetry.scanInterval": {
          "type": "number",
          "default": 90,
          "minimum": 30,
          "maximum": 86400,
          "description": "Telemetry scan interval in seconds (30-86400)"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src/",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "test:unit": "npm run compile && mocha 'out/test/unit/**/*.test.js'",
    "package": "vsce package"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/chai": "^5.2.3",
    "@types/glob": "^9.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.0.0",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.104.0",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "sinon": "^21.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}